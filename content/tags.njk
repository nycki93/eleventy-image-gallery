---
pagination:
  data: collections.$galleryTagList
  size: 1
  alias: tag
permalink: /tagged/{{ tag }}/
---
<h1>Tagged {{ tag }}</h1>
<ul>
  {% for post in collections.$gallery | tagged(tag) %}
    <li><a href="{{ post.url }}">{{ post.data.title or post.page.fileSlug }}</a></li>
  {% endfor %}
</ul>
